  "version": "0.2.0",
  "configurations": [
    {
      "name": "cca (debug)",
      "cwd": "",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "preLaunchTask": "connect_mobile",
      "env": {
        "APP_ENV": "development",
        "SERVER_HOST": "localhost",
        "SERVER_PROTOCOL": "development",
        "SERVER_PORT": "http",
        "APP_ID":"development",
        "APP_SECRET":"1234"
      }
    },
    {
      "name": "cca (debug using remote server)",
      "cwd": "",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "preLaunchTask": "connect_mobile",
      "args": ["--dart-define", "RELEASE_MODE=true"],
      "env": {
        "APP_ENV": "development",
        "SERVER_HOST": "localhost",
        "SERVER_PROTOCOL": "development",
        "SERVER_PORT": "http"
      }
    },
  ]
}
